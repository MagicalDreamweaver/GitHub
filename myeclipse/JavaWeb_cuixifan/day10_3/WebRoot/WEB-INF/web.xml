<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <servlet-name>AServlet</servlet-name>
    <servlet-class>com.webrequest2.AServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>OneServlet</servlet-name>
    <servlet-class>com.request.forward.OneServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TwoServlet</servlet-name>
    <servlet-class>com.request.forward.TwoServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ThreeServlet</servlet-name>
    <servlet-class>com.servlet.include.ThreeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Four</servlet-name>
    <servlet-class>com.servlet.include.Four</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>FourServlet</servlet-name>
    <servlet-class>com.servlet.include.FourServlet</servlet-class>
  </servlet>






  <servlet-mapping>
    <servlet-name>AServlet</servlet-name>
    <url-pattern>/AServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>OneServlet</servlet-name>
    <url-pattern>/OneServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TwoServlet</servlet-name>
    <url-pattern>/TwoServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ThreeServlet</servlet-name>
    <url-pattern>/ThreeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Four</servlet-name>
    <url-pattern>/Four</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FourServlet</servlet-name>
    <url-pattern>/FourServlet</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
